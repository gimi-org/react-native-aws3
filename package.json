{
  "name": "react-native-aws3",
  "version": "0.0.12",
  "description": "Pure JavaScript react native library for uploading to AWS S3",
  "author": {
    "name": "Ben Reinhart"
  },
  "keywords": [
    "react",
    "react-native",
    "react-component",
